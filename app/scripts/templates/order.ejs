<i class="right floated inverted teal circular edit icon list-btn"></i>
<div class="field fix-checkbox-position">
    <div class="ui toggle checkbox">
        <input id="<%= objectId %>" <%= done ? 'checked' : '' %> type="checkbox">
        <label for="<%= objectId %>"></label>
    </div>
</div>
<div class="content">
    <div class="header"><%= title %></div>
    <div class="col-xs-10 edit">
        <div class="content">
            <div class="ui fluid icon input">
                <input type="text" placeholder="Place your order">
            </div>
        </div>
    </div>
</div>
